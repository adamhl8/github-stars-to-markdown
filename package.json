  },
  "dependencies": {
    "ts-explicit-errors": "^4.0.1"
  },
  "devDependencies": {
    "@adamhl8/configs": "^0.17.5",
    "@biomejs/biome": "^2.2.4",
    "@types/bun": "^1.2.23",
    "knip": "^5.64.1",
    "markdown-toc": "^1.2.0",
    "typescript": "^5.9.2"
  }
}
